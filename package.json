{
  "name": "templator",
  "version": "2.6.1",
  "description": "template editor based on Silex website builder",
  "main": "server/index.js",
  "engines": {
    "node": ">=8.10.0 <=11.15.0"
  },
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "electron .",
    "build": "npm run build:components && npm run build:html",
    "build:components": "mkdir -p pub/components && prodotype components/ pub/components",
    "build:html": "mkdir -p pub/ && pug client/index.pug --out pub/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexoyo/templator.git"
  },
  "keywords": [
    "silex",
    "website",
    "builder",
    "free",
    "foss",
    "website builder",
    "diy",
    "jekyll",
    "11ty",
    "liquid",
    "static"
  ],
  "author": "lexoyo",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/silexlabs/Silex/issues"
  },
  "homepage": "https://www.silex.me",
  "dependencies": {
    "silex-website-builder": "silexlabs/Silex#develop"
  },
  "devDependencies": {
    "prodotype": "^1.0.22",
    "pug-cli": "^1.0.0-alpha6",
    "electron": "9.0.0",
    "electron-builder": "22.7.0",
    "electron-rebuild": "1.11.0"
  }
}
